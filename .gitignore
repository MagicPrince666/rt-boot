#
# NOTE! Don't add files that are generated in specific
# subdirectories here. Add them in the ".gitignore" file
# in that subdirectory instead.
#
# Normal rules
#
.*
*.o
*.o.*
*.a
*.su
*.mod.c
*.i
*.lst
*.order
*.elf
*.swp
*.bin
*.patch
*.cfgtmp
*.prebuild

# host programs on Cygwin
*.exe

# lzma host util
host_util/lzma/lzma

#
# Top-level generic files
#
rt-boot/System.map
rt-boot/u-boot*
rt-boot/bootstrap
rt-boot/bootstrap.map

# MD5 in bin
/bin/*.md5

# Symbolic links and some compilation results from tools
rt-boot/tools/crc32.c
rt-boot/tools/environment.c
rt-boot/tools/envcrc
rt-boot/tools/mkimage
rt-boot/tools/mips-relocs
rt-boot/include/config.h
rt-boot/include/config.mk
rt-boot/include/version_autogenerated.h

#
# git files that we don't want to ignore even it they are dot-files
#
!.gitignore
!.mailmap

# stgit generated dirs
patches-*
.stgit-edit.txt

# quilt's files
patches
series

# gdb files
.gdb_history

# cscope files
cscope.*

# tags files
tags
ctags
etags

# gnu global files
GPATH
GRTAGS
GSYMS
GTAGS

*.orig
*~
\#*#
